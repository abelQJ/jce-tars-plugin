{
    "scripts": {
        "vscode:prepublish": "cd client && npm run update-vscode && cd .. && npm run compile",
        "compile": "tsc -b",
        "watch": "tsc -b -w ",
        "postinstall": "cd client && npm install && cd ../server && npm install && cd .."
    },
    "devDependencies": {
        "@types/mocha": "^5.2.0",
        "@types/node": "^8.0.0",
        "tslint": "^5.11.0",
        "typescript": "^3.1.3"
    },
    "name": "jce-plugin",
    "description": "A plugin for jce file of TAF framework.",
    "author": "abelqjli",
    "license": "MIT",
    "version": "3.0.2",
    "publisher": "abelqjli",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.23.0"
    },
    "activationEvents": [
        "onLanguage:jce"
    ],
    "main": "./client/out/extension",
    "categories": [
        "Programming Languages",
        "Snippets",
        "Other"
    ],
    "keywords": [
        "jce",
        "taf",
        "tars",
        "pb",
        "tencent"
    ],
    "homepage": "https://github.com/abelQJ/jce-tars-plugin",
    "repository": {
        "type": "git",
        "url": "https://github.com/abelQJ/jce-tars-plugin.git"
    },
    "bugs": {
        "url": "https://github.com/abelQJ/jce-tars-plugin/issues"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "jce-plugin configuration",
            "properties": {
                "jce.check_grammar": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": true,
                    "description": "Controls the jce grammar check."
                }
            }
        },
        "languages": [
            {
                "id": "jce",
                "aliases": [
                    "jce",
                    "tars"
                ],
                "extensions": [
                    ".jce",
                    ".tars"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jce",
                "scopeName": "source.jce",
                "path": "./syntaxes/jce.tmLanguage.json"
            }
        ]
    }
}
